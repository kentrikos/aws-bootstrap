{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "S3AmazonS3FullAccessLimited",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:ListAllMyBuckets",
                "s3:GetBucketLocation",
                "s3:PutBucketTagging",
                "s3:CreateBucket",
                "s3:ListBucket",
                "s3:DeleteObject",
                "s3:HeadBucket",
                "s3:DeleteBucket",
                "s3:PutBucketVersioning",
                "s3:GetBucketCors"
            ],
            "Resource": "*"
        }
    ]
}
